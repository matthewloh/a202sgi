<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/light_pink">

    <Button
        android:id="@+id/BackToEventPage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Event"/>

    <View
        android:id="@+id/view3"
        android:layout_width="342dp"
        android:layout_height="511dp"
        android:background="@color/white"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="110dp" />

    <ImageButton
        android:id="@+id/TasterEvent"
        android:layout_width="248dp"
        android:layout_height="90dp"
        android:layout_marginEnd="370dp"
        android:src="@drawable/Taster"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/view3"
        tools:layout_editor_absoluteY="138dp" />


    <TextView
        android:layout_width="320dp"
        android:layout_height="298dp"
        android:layout_marginBottom="6dp"
        android:gravity="center"
        android:text="Example Info"
        android:textStyle="bold"
        android:typeface="sans"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        tools:layout_editor_absoluteX="43dp" />

    <View
        android:id="@+id/white_background"
        android:layout_width="432dp"
        android:layout_height="71dp"
        android:background="#FFFFFF"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="657dp" />


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="143dp"
        android:layout_height="148dp"
        android:src="@drawable/BigIcon"
        tools:layout_editor_absoluteX="134dp"
        tools:layout_editor_absoluteY="580dp" />

    <ImageButton
        android:id="@+id/Homepage1"
        android:layout_width="44dp"
        android:layout_height="52dp"
        android:src="@drawable/homeIcon"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="663dp" />

    <ImageButton
        android:id="@+id/Homepage2"
        android:layout_width="44dp"
        android:layout_height="52dp"
        android:src="@drawable/CalendarIcon"
        tools:layout_editor_absoluteX="267dp"
        tools:layout_editor_absoluteY="663dp" />

    <ImageButton
        android:id="@+id/Homepage3"
        android:layout_width="44dp"
        android:layout_height="52dp"
        android:src="@drawable/notificationIcon"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="663dp" />

    <ImageButton
        android:id="@+id/Homepage4"
        android:layout_width="44dp"
        android:layout_height="52dp"
        android:src="@drawable/people"
        tools:layout_editor_absoluteX="351dp"
        tools:layout_editor_absoluteY="663dp" />


</androidx.constraintlayout.widget.ConstraintLayout>